{
    "manifest_version": 3,
    "name": "Raisin add-on",
    "description": "Add-on to enrich your Raisin experience.",
    "version": "0.1.0",
    "icons": {
        "48": "img/icon-48.jpg",
        "128": "img/icon-128.jpg"
    },
    "content_scripts": [
        {
            "js": [
                "scripts/i18n.js",
                "scripts/content.js"                
            ],
            "matches": [
                "https://www.raisin.nl/savingglobal/*"
            ]
        }
    ]
}