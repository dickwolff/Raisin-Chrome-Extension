{
    "manifest_version": 3,
    "name": "__MSG_name__",
    "description": "__MSG_description__",
    "version": "0.6.6",
    "icons": {
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png"
    },
    "default_locale": "en",
    "permissions": [
        "storage"
    ],
    "content_scripts": [
        {
            "js": [
                "contentscript.js"
            ],
            "matches": [
                "https://www.raisin.com/*",
                "https://www.raisin.co.uk/*",
                "https://www.raisin.es/*",
                "https://www.raisin.fr/*",
                "https://www.raisin.ie/*",
                "https://www.raisin.nl/*",                
                "https://www.weltsparen.at/*",
                "https://www.weltsparen.de/*"
            ]
        }
    ]
}