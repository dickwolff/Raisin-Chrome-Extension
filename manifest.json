{
    "manifest_version": 3,
    "name": "Raisin add-on",
    "description": "Base Level Extension",
    "version": "1.0",
    "host_permissions": [
        "https://www.raisin.nl/",
        "https://api2.weltsparen.de/"
      ],
    "content_scripts": [
        {
            "js": [
                "scripts/content.js"
            ],
            "matches": [
                "https://www.raisin.nl/savingglobal/*"
            ]
        }
    ]
}